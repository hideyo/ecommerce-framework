{
    "name": "hideyo/ecommerce-framework",
    "type": "library",
    "description": "A simple API extension for DateTime.",

    "homepage": "http://hideyo.io",
    "license": "GPL-3.0+",
    "require": {
        "php": ">=5.6.4",
        "fideloper/proxy": "~3.3",        
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "edvinaskrucas/notification": "5.*",
        "cviebrock/eloquent-sluggable": "^4.1",
        "yajra/laravel-datatables-oracle": "~6.0",
        "barryvdh/laravel-debugbar": "^2.1",
        "laravelcollective/html": "^5.2",
        "baum/baum": "~1.1",
        "intervention/image": "~2.3",
        "intervention/imagecache": "^2.3",
        "felixkiss/uniquewith-validator": "2.*",
        "nesbot/carbon": "~1.14",
        "predis/predis": "~1.1@dev",
        "maatwebsite/excel": "~2.1.0",
        "elasticquent/elasticquent": "1.0.6",
        "khill/lavacharts": "~3.0",
        "tylercd100/lern": "^3.6",
        "unisharp/laravel-ckeditor": "^4.5",
        "unisharp/laravel-filemanager": "^1.6",
        "hisorange/browser-detect": "2.*",
        "flynsarmy/db-blade-compiler": "5.4.2",
        "laravel/passport": "^4.0@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "Hideyo\\": "src/Hideyo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.23-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit; ./vendor/bin/php-cs-fixer fix -v --diff --dry-run;",
        "phpunit": "./vendor/bin/phpunit;",
        "phpcs": "./vendor/bin/php-cs-fixer fix -v --diff --dry-run;"
    }
}
